for b in batch]).to(device)\n    lbls = torch.stack([b[1] for b in batch]).to(device)\n    return pvs, lbls\n\ntrain_loader = DataLoader(train_ds, batch_size=8, sampler=sampler,\n                          pin_memory=True, num_workers=4, collate_fn=collate_fn)\nval_loader   = DataLoader(val_ds,   batch_size=16, shuffle=False,\n                          pin_memory=True, num_workers=4, collate_fn=collate_fn)\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-05-27T07:53:32.313114Z","iopub.execute_input":"2025-05-27T07:53:32.313337Z","iopub.status.idle":"2025-05-27T07:53:35.007056Z","shell.execute_reply.started":"2025-05-27T07:53:32.313317Z","shell.execute_reply":"2025-05-27T07:53:35.006289Z"}},"outputs":[],"execution_count":4},{"cell_type":"code","source":"import torch\nfrom torch.utils.data import DataLoader\nfrom sklearn.metrics import roc_auc_score\nimport time\n\n# 1) Put model on device\nmodel = CXRMultiLabel(vision_encoder, num_labels=len(label_cols))\nmodel.pos_weight = pos_weight.to(device)\nmodel.to(device)\n\n# 2) DataLoaders\ndef collate_batch(batch):\n    pvs  = torch.stack([b[0] for b in batch])   # CPU\n    lbls = torch.stack([b[1] for b in batch])   # CPU\n    return pvs, lbls\n\ntrain_loader = DataLoader(\n    train_ds, batch_size=8, sampler=sampler,\n    num_workers=4, pin_memory=True, collate_fn=collate_batch\n)\nval_loader = DataLoader(\n    val_ds, batch_size=16, shuffle=False,\n    num_workers=4, pin_memory=True, collate_fn=collate_batch\n)\n\n# 3) Optimizer & LR scheduler\noptimizer = torch.optim.AdamW(model.parameters(), lr=3e-5)\ntotal_steps = len(train_loader) * 5  # 5 epochs\nscheduler = torch.optim.lr_scheduler.CosineAnnealingLR(optimizer, T_max=total_steps)\n\n# 4) Training loop\nfor epoch in range(1, 6):\n    model.train()\n    epoch_loss = 0.0\n    t0 = time.time()\n    for pv, lbl in train_loader:\n        # move to GPU here, in the main process\n        pv, lbl = pv.to(device, non_blocking=True), lbl.to(device, non_blocking=True)\n        out = model(pv, lbl)\n        loss = out[\"loss\"]\n        optimizer.zero_grad()\n        loss.backward()\n        optimizer.step()\n        scheduler.step()\n        epoch_loss += loss.item()\n        if step % 100 == 0:\n            print(f\"Epoch {epoch} • step {step}/{len(train_loader)} • loss {epoch_loss/step:.4f}\")\n    print(f\"→ Epoch {epoch} finished in {time.time()-t0:.1f}s, avg loss {epoch_loss/len(train_loader):.4f}\")\n\n    # 5) Validation & per-class AUROC\n    model.eval()\n    all_logits, all_labels = [], []\n    with torch.no_grad():\n        for pv, lbl in val_loader:\n            logits = model(pv)[\"logits\"]\n            all_logits.append(logits.cpu())\n            all_labels.append(lbl.cpu())\n    all_logits = torch.cat(all_logits)\n    all_labels = torch.cat(all_labels)\n\n    aurocs = []\n    for i, cname in enumerate(label_cols):\n        try:\n            auc = roc_auc_score(all_labels[:, i].numpy(), all_logits[:, i].numpy())\n        except ValueError:\n            auc = float(\"nan\")  # no positives in val\n        aurocs.append(auc)\n    print(\"Validation AUROC per class:\")\n    for cname, auc in zip(label_cols, aurocs):\n        print(f\"  {cname:>20}: {auc:.3f}\")\n    print(\"-\"*40)\n\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-05-27T07:53:35.007870Z","iopub.execute_input":"2025-05-27T07:53:35.008107Z","iopub.status.idle":"2025-05-27T07:53:39.711642Z","shell.execute_reply.started":"2025-05-27T07:53:35.008090Z","shell.execute_reply":"2025-05-27T07:53:39.710258Z"}},"outputs":[{"traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mOutOfMemoryError\u001b[0m                          Traceback (most recent call last)","\u001b[0;32m/tmp/ipykernel_35/1539111642.py\u001b[0m in \u001b[0;36m<cell line: 0>\u001b[0;34m()\u001b[0m\n\u001b[1;32m     37\u001b[0m         \u001b[0;31m# move to GPU here, in the main process\u001b[0m